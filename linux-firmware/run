#!/bin/sh
NAME="linux-firmware"
RELEASE="1"
VERSION="20221109"
SOURCES="https://git.kernel.org/pub/scm/linux/kernel/git/firmware/linux-firmware.git/snapshot/linux-firmware-$VERSION.tar.gz"
SHA256SUM="c23299426d190986ba6eb52a3b22c4e4383b3a055903ff3967fb17afad3edb9b  linux-firmware-20221109.tar.gz"
DESCRIPTION="Linux firmware files"

build() {
    cd linux-firmware-$VERSION || exit 1
    make install
}
