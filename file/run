#!/bin/sh
NAME="file"
RELEASE="1"
VERSION="5.45"
SOURCES="https://astron.com/pub/file/file-$VERSION.tar.gz"
SHA256SUM="fc97f51029bb0e2c9f4e3bffefdaf678f0e039ee872b9de5c002a6d09c784d82  file-5.45.tar.gz"
DESCRIPTION="File tool"
build() {
    ./configure --prefix=/usr
    make
}

install() {
    make DESTDIR=$ROOT install
}
