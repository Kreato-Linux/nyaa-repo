#!/bin/sh
NAME="libressl"
VERSION="3.5.3"
SOURCES="https://ftp.openbsd.org/pub/OpenBSD/LibreSSL/libressl-$VERSION.tar.gz"
SHA256SUM="SKIP"
DESCRIPTION="TLS library"

build() {
    tar -xvf libressl-$VERSION.tar.gz
    cd libressl-$VERSION || exit 1
    ./configure
    make -j$(nproc)
    make DESTDIR="$ROOT" install
}

