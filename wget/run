#!/bin/sh
NAME="wget"
RELEASE="2"
VERSION="1.21.4"
SOURCES="https://ftp.gnu.org/gnu/wget/wget-$VERSION.tar.gz"
DEPENDS="openssl"
SHA256SUM="3683619a5f50edcbccb1720a79006fa37bf9b9a255a8c5b48048bc3c7a874bd9"
DESCRIPTION="Network utility to retrieve files from the Web"
build() {
    ./configure --prefix=/usr --with-ssl=openssl
    make
}

package() {
    make install
}
