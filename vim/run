#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1671"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
SHA256SUM="5214a63464b39d4bc52f9bddf0634e4a2decd751650b3cff3f005c9539e3cd33  v9.0.1671.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

install() {
    make install
}
