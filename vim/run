#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1679"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
DEPENDS=""
SHA256SUM="d285eafe22ea07bbb0765fd547a6ee1cd58a62fc3aacf49b1c8e6f4e6e3bd9d2  v9.0.1679.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

package() {
    make install
}
