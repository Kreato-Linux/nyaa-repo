#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1695"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
DEPENDS=""
SHA256SUM="fda06bf8f4d0bb173fa05d23dc1733c855d62bb76560b7b1046910b78494bb87  v9.0.1695.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

package() {
    make install
}
