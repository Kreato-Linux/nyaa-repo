#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1665"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
SHA256SUM="eda541361f9fd47d84d7e3ab1f9ebee4db06ed13bfca95c54d5bab2b3b97809a  v9.0.1665.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

install() {
    make install
}
