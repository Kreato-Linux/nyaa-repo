#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1672"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
SHA256SUM="d0067412973a6d7bbc1072e424e276d73f87eefa10478fe3d0076a45c47b03ab  v9.0.1672.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

install() {
    make install
}
