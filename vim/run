#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1910"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
DEPENDS=""
SHA256SUM="c795e56f5c2be6bd97a36f15953897f902447d6466b1b17ea8001c63063739b6"
