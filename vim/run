#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1683"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
DEPENDS=""
SHA256SUM="abe9b708a153c1abb5daa286a0941a9bd3765c701312f72aad5d0d146772bbba  v9.0.1683.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

package() {
    make install
}
