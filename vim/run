#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1670"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
SHA256SUM="7f02b5341d1f66aa27ecc5ebf3f911f798bf5d205ce12fdb5073d17539953fcb  v9.0.1670.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

install() {
    make install
}
