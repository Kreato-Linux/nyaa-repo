#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1682"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
DEPENDS=""
SHA256SUM="73eaf2d39882de45604dced91d164988feec0cf19215cf517c3a0a82467f20fc  v9.0.1682.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

package() {
    make install
}
