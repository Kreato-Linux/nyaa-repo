#!/bin/sh
NAME="vim"
RELEASE="1"
VERSION="9.0.1700"
SOURCES="https://github.com/vim/vim/archive/refs/tags/v$VERSION.tar.gz"
DEPENDS=""
SHA256SUM="90a167fac8f66ab327a9d7a6c478c18a39925ba9ed6aee870d4ea3d7f7c1629a  v9.0.1700.tar.gz"
DESCRIPTION="Vi Improved"

build() {
    make -j$(nproc)
}

package() {
    make install
}
