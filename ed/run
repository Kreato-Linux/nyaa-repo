#!/bin/sh
NAME="ed"
RELEASE="1"
VERSION="1.19"
SOURCES="https://ftp.gnu.org/gnu/ed/ed-$VERSION.tar.lz"
BUILD_DEPENDS="lzip"
SHA256SUM="ce2f2e5c424790aa96d09dacb93d9bbfdc0b7eb6249c9cb7538452e8ec77cd48"
DESCRIPTION="A line-oriented text editor"

build() {
    ./configure --prefix=/usr
    make
}

package() {
    make install 
}
